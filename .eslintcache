azyUser = lazy(() => import(\"./User\"));\nconst LazyContacts = lazy(() => import(\"./Contacts\"));\n\n/* \nin \"index.js\", the Mobx store object is declared and passed to the \n\"new UniversalRouter(routes, {context})\"  via the object \"context={store:store}\"\nHere is the skeleton of \"routes\" waiting for the context to be declared (or not)\n*/\n\nexport const routes = [\n  {\n    path: \"\",\n    async action({ store, next }) {\n      const component = await next();\n      return (\n        component && (\n          <Suspense fallback={<Spinner />}>\n            <LazyLayout store={store}>{component}</LazyLayout>\n          </Suspense>\n        )\n      );\n    },\n    children: [\n      {\n        path: \"/\",\n        // -> testing \"static general props\" passing with ctx & resolve\n        action: async ({ store, mode }) => {\n          return (\n            <Suspense fallback={spin()}>\n              <LazyHome store={store} mode={mode} />;\n            </Suspense>\n          );\n        },\n      },\n      {\n        path: \"/about\",\n        async action({ store }) {\n          // note: \"info\" is just a trial to see how data is passed via the context object\n          return (\n            <Suspense fallback={spin()}>\n              <LazyAbout store={store} />;\n            </Suspense>\n          );\n        },\n      },\n      {\n        path: \"/signinform\",\n        async action({ store }) {\n          return (\n            <Suspense fallback={spin()}>\n              <LazySignInForm store={store} />;\n            </Suspense>\n          );\n        },\n      },\n      {\n        path: \"/addusers\",\n        async action({ store, mode }) {\n          if (mode === process.env.REACT_APP_MODE)\n            return (\n              <Suspense fallback={spin()}>\n                <LazyNewUsersForm store={store} />;\n              </Suspense>\n            );\n          else {\n            // return { redirect: \"/\", from: context.pathname };\n            return (\n              <Suspense fallback={spin()}>\n                <LazyHome store={store} />;\n              </Suspense>\n            );\n          }\n        },\n      } /*\n      {\n        path: \"/addusers\",\n        async action({ store, mode, next }) {\n          if (mode === null) {\n            return { redirect: \"/about\" };\n          }\n          return next();\n        },\n        children: [\n          {\n            path: \"/\",\n            async action({ store }) {\n              return (\n                <Suspense fallback={spin()}>\n                  <LazyNewUsersForm store={store} />;\n                </Suspense>\n              );\n            },\n          },\n        ],\n      },*/,\n      {\n        path: \"/contacts\",\n        children: [\n          {\n            path: \"\",\n            // the \"action\" below is from Universal Router\n            async action({ store }) {\n              fetchUsers() // the \"action\" below is from Mobx\n                .then(action((res) => store.addUsers(res)));\n              return (\n                <Suspense fallback={spin()}>\n                  <LazyContacts store={store} />;\n                </Suspense>\n              );\n            },\n          },\n          {\n            path: \"/:email\",\n            async action(context) {\n              // <=> ({ store,mode }, { email }) instead of context\n              // where \"const email = context.params.email\" // cf UR docs\n\n              return (\n                <Suspense fallback={spin()}>\n                  <LazyUser\n                    email={context.params.email}\n                    store={context.store}\n                    mode={context.mode}\n                  />\n                  ;\n                </Suspense>\n              );\n            },\n          },\n        ],\n      },\n    ],\n  },\n];\n\n// const context = {\n//   mode: \"admin\", <- just a trial\n//   store,\n// };\n\n// export default new UniversalRouter(routes, { context });\n","/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/Contacts.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/context/statectx.jsx",["134","135"],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/utils/store.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/utils/history.js",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/User.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/reportWebVitals.js",[],["136","137"],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/utils/initFacebookSdk.js",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/SimpleModal.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/SignInForm.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/utils/theme.js",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/SgnButton.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/EditUser.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/SignInModal.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/NewUsersForm.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/index.js",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/Error.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/Spinner.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/utils/fetchUsers.js",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/utils/FBIcon.js",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/FBConnect.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/FBButton.jsx",["138"],"import React, { useState } from \"react\";\n// import FBIcon from \"../utils/FBIcon\";\n// import history from \"../utils/history\";\n\nimport { Button } from \"@material-ui/core\";\nimport { action } from \"mobx\";\nimport { observer } from \"mobx-react-lite\";\nimport store from \"../utils/store\";\n\nconst FBButton = observer(() => {\n  const [user, setUser] = useState(\"\");\n\n  const statusCallback = async (response) => {\n    const fbToken = response.authResponse.accessToken;\n    if (response.status !== \"connected\") {\n      console.log(\"not logged in\");\n      return;\n    }\n    if (response.status === \"connected\") {\n      return window.FB.api(\n        \"/me\",\n        { fields: \"name, email\" },\n        action(function (response) {\n          setUser({ name: response.name, email: response.email });\n          store.setCurrent({\n            name: response.name,\n            email: response.email,\n            pwd: fbToken,\n            signed: true,\n            fb: true,\n          });\n          store.toggleSgn();\n        })\n      );\n    }\n  };\n\n  const FBLogin = action(async () => {\n    window.FB.getLoginStatus(\n      action(async (response) => {\n        const token = response?.authResponse?.accesToken;\n        if (!response.authResponse) {\n          window.FB.login(\n            (response) => {\n              statusCallback(response);\n            },\n            { scope: \"email\" }\n          );\n          // history.push({ pathname: \"/\" });\n        } else {\n          return window.FB.api(\n            \"/me\",\n            { fields: \"name, email\" },\n            action(function (response) {\n              setUser({ name: response.name, email: response.email });\n              store.setCurrent({\n                name: response.name,\n                email: response.email,\n                pwd: token,\n                signed: true,\n                fb: true,\n              });\n              store.toggleSgn();\n            })\n          );\n          // setUser(\"\");\n          // store.setCurrent({});\n          // store.isSignedIn = false;\n          // return window.FB.logout();\n        }\n      })\n    );\n  });\n\n  return (\n    <>\n      <Button\n        onClick={FBLogin}\n        fullWidth\n        disabled={store.current.signed}\n        variant=\"outlined\"\n        style={{\n          backgroundColor: store.current.signed ? \"#997b3b\" : \"#3b5999\",\n        }}\n      >\n        {store.current.signed\n          ? `Welcome ${store.current.name}`\n          : \"Login with Facebook\"}\n      </Button>\n    </>\n  );\n});\n\nexport default FBButton;\n","/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/DividerWithText.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/ActionAlerts.jsx",[],"/Users/utilisateur/code/ndrean/learn-javascript/navbar/src/component/ModeSwitch.jsx",[],{"ruleId":"139","replacedBy":"140"},{"ruleId":"141","replacedBy":"142"},{"ruleId":"139","replacedBy":"143"},{"ruleId":"141","replacedBy":"144"},{"ruleId":"145","severity":1,"message":"146","line":3,"column":10,"nodeType":"147","messageId":"148","endLine":3,"endColumn":18},{"ruleId":"145","severity":1,"message":"149","line":10,"column":10,"nodeType":"147","messageId":"148","endLine":10,"endColumn":15},{"ruleId":"145","severity":1,"message":"150","line":10,"column":17,"nodeType":"147","messageId":"148","endLine":10,"endColumn":25},{"ruleId":"139","replacedBy":"151"},{"ruleId":"141","replacedBy":"152"},{"ruleId":"145","severity":1,"message":"153","line":11,"column":10,"nodeType":"147","messageId":"148","endLine":11,"endColumn":14},"no-native-reassign",["154"],"no-negated-in-lhs",["155"],["154"],["155"],"no-unused-vars","'observer' is defined but never used.","Identifier","unusedVar","'state' is assigned a value but never used.","'setState' is assigned a value but never used.",["154"],["155"],"'user' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]